.view-wrap{
  margin-top:-15px;
  padding:0px;
  color:#777;
}
.nearbg-title {
  height: 40px;
  line-height: 40px;
  border-bottom: 1px solid #f0f0f0;
  font-size: 14px;
  margin: 0px 10px;
}

.nearbg-title-r {
  color: #9d9d9d;
}

.nearbg {
  padding: 10px 10px;
  height: 40px;
  border-bottom: 1px solid #f0f0f0;
  font-size: 14px;
}

.nearbg image {
  width: 45px;
  height: 45px;
  border-radius: 25px;
  -webkit-border-radius: 25px;
  -moz-border-radius: 25px;
}

.nearbg-infor {
  padding-left: 5px;
}

.nearbg-canquan {
  color: #e02e24;
  font-weight: bold;border: 1px solid #e02e24;
  
  height: 30px;margin-top: 3px;
  width: 60px;
  text-align: center;
  line-height: 30px;
  border-radius: 8px;
  margin-left: 15px;
  font-size: 14px;
}
.fl {
  float: left;
}

.fr {
  float: right;
}

.clear {
  clear: both;
}